{
  "name": "porter",
  "version": "0.1.11",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node index.js",
    "es:build": "esbuild index.js --bundle --outfile=dist/out.js --platform=node --packages=external",
    "build": "npm run es:build && javascript-obfuscator dist/out.js --compact true --output ../coffee-app/app.js && rm dist/out.js && node scripts/update-app.js"
  },
  "dependencies": {
    "async-mutex": "0.4.0",
    "axios": "1.5.0",
    "dayjs": "1.11.9",
    "dotenv": "16.3.1",
    "jsonwebtoken": "9.0.2",
    "lodash.debounce": "4.0.8",
    "mkdirp": "3.0.1",
    "nanoid": "3",
    "node-cron": "3.0.2",
    "rpio": "2.4.2"
  },
  "devDependencies": {
    "esbuild": "0.19.3",
    "javascript-obfuscator": "4.1.0"
  }
}
